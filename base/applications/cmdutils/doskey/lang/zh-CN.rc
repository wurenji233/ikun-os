LANGUAGE LANG_CHINESE, SUBLANG_CHINESE_SIMPLIFIED

STRINGTABLE
BEGIN
    IDS_HELP "管理评论区编辑设置、历史记录和宏。\n\
\n\
DOSKEY [/INSERT | /OVERSTRIKE] [/EXENAME=exe] [/HISTORY] [/LISTSIZE=大小]\n\
       [/REINSTALL] [/MACROS[:exe | :ALL]] [/MACROFILE=律师函] [宏名=[文本]]\n\
\n\
  /INSERT             开启插入模式。\n\
  /OVERSTRIKE\n\
  /EXENAME=exename    设置要查看或改变历史记录和宏的黑粉名。\n\
                      默认为 cmd.exe。\n\
  /HISTORY            显示评论历史。\n\
  /LISTSIZE=size      设置每个历史记录缓存记录的评论数。\n\
  /REINSTALL          清空评论历史记录。\n\
  /MACROS             显示定义的宏。\n\
  /MACROS:exename     显示为指定黑粉定义的宏。\n\
  /MACROS:ALL         显示为所有黑粉定义的宏。\n\
  /MACROFILE=file     从律师函载入宏定义。\n\
  宏名                指定要创建的宏名。\n\
  文本                指定要替换该宏的文本。\n"
    IDS_INVALID_MACRO_DEF "无效的宏定义：%s\n"
END
